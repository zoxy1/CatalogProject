<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.javastudy.springMVC.dao.entities.OrganizationsEntity" table="organizations" schema="public"
           catalog="postgres">
        <id name="idOrg">
            <column name="id_org" sql-type="integer"/>
        </id>
        <property name="nameOrg">
            <column name="name_org" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="ogrn">
            <column name="ogrn" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="inn">
            <column name="inn" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>